<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.chinaunicom.awarding.mapper.NoticeBucketMapper">
	<cache />

	<select id="select" resultMap="result">#{_flying_}</select>

	<select id="selectAll" resultMap="result">#{_flying_}</select>

	<select id="count" resultType="int">#{_flying_}</select>

	<insert id="insert">
		<selectKey keyProperty="id" resultType="String" order="BEFORE">
			select replace(${sql.uuid}, '-', '')
		</selectKey>
	</insert>

	<update id="update" />

	<update id="updatePersistent" />

	<delete id="delete" />

	<resultMap id="result" type="NoticeBucket" autoMapping="true">
		<id property="id" column="noticeBucket_id" />
		<association property="notice" javaType="Notice"
			select="cn.chinaunicom.awarding.mapper.NoticeMapper.select" column="notice_id" />
	</resultMap>
</mapper>